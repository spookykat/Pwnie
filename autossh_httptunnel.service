[Unit]
Description=AutoSSH tunnel for httptunnel port 1234
After=network.target     
Requires=stunnel.service

[Service]
Enviroment="AUTOSSH_GATETIME=0"   
ExecStart=/usr/bin/autossh -p 1234 -M 0 -N -R 10222:localhost:22 root@localhost
Restart=always
RestartSec=5

[Install]
WantedBy=multi-user.target